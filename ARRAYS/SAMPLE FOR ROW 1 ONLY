#include <iostream>
using namespace std;

void display(int markse[5]) {
    cout << "Enter the marks: ";
    
    for (int i = 0; i < 5; i++) {
        cin >> markse[i];
        
    }
    cout << "The marks are: ";
    for (int i = 0; i < 5; i++){
        cout << markse[i] << " ";
    }
    
    
    cout << endl;
}

int main() {
    int markse[5];
    display(markse);
    return 0;
}
