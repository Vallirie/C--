#include <iostream>
using namespace std;

// Function definition to display marks
void display(int marks[5]) {
    cout << "The marks are: ";
    for (int i = 0; i < 5; i++) {
        cout << marks[i] << " ";
    }
    cout << endl;
}

int main() {
    int marks[5] = {88, 76, 90, 61, 69};
    display(marks);
    return 0;
}
